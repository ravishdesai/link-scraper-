{
    "manifest_version": 2,
    "name": "Link scraper",
    "description": "Scraps all the links from a web page",
    "version": "1.0.0",
    "icons": {
        "16": "get_started16.png",
        "48": "get_started48.png",
        "128": "get_started128.png"
    },
    "browser_action": {
        "default_icon": "get_started16.png"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["script.js"]
    }],
    "permissions": [
        "activeTab"
    ]


}